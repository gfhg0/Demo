<div class="row">
  <% @musics.each do |music| %>
    <div class="col-sm-6 col-md-3">
      <div class="img-thumbnail">
      <%= link_to image_tag(music.image.url(:medium)), music_path(music) %><br>
      	<div class="music-name">
        	<%= music.artist %>
      	</div>
      	<div class="music-title">
      		<%= music.title %>
      	</div>
    </div>
    </div>
  <% end %>
</div>