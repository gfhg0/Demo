<%= form_for @music, html: { multipart: true } do |f| %>
  <% if @music.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@music.errors.count, "error") %> prohibited this music from being saved:</h2>
      <ul>
      <% @music.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<div class="row">
  <div class="col-xs-4">
      <div class="form-group">
        <%= f.label :artist %><br>
        <%= f.text_field :artist, class: 'form-control' %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-4">
     <div class="form-group">
      <%= f.label :title %><br>
      <%= f.text_field :title, class: 'form-control'  %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-4">
     <div class="form-group">
      <%= f.label :year %><br>
      <%= f.number_field :year, class: 'form-control'  %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-4">
      <div class="form-group">
        <%= f.label :label %><br>
        <%= f.text_field :label, class: 'form-control'  %>
      </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-4">
      <div class="form-group">
    <%= f.label :genere %><br>
    <%= f.text_field :genere, class: 'form-control'  %>
  </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-4">
     <div class="form-group">
    <%= f.label :format %><br>
    <%= f.text_field :format, class: 'form-control'  %>
  </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-4">
    <div class="form-group">
     <%= f.label :rate %><br>
      <%= f.number_field :rate, class: 'form-control'  %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-xs-4">
     <div class="form-group">
      <%= f.label :notes %><br>
      <%= f.text_field :notes, class: 'form-control'  %>
     </div>
  </div>
</div> 

    <%= f.label :image %><br>
    <%= f.file_field :image %><br/>

<div class="form-group">
  <%= f.submit 'Create', :class => 'btn btn-default' %>
</div>
  
<% end %>
